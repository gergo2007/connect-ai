{
    "name": "@luvio/jwt-manager",
    "version": "5.30.0",
    "description": "Luvio Next generic JWT manager",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce-experience-platform-emu/luvio-next.git",
        "directory": "packages/@luvio/jwt-manager"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "main": "dist/jwt-manager.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "rollup --config rollup.config.js",
        "clean": "rm -rf dist",
        "test": "vitest run",
        "test:size": "bundlesize",
        "watch": "yarn build --watch"
    },
    "dependencies": {
        "jwt-decode": "~3.1.2"
    },
    "devDependencies": {
        "@luvio/utils": "5.30.0"
    },
    "bundlesize": [
        {
            "path": "./dist/jwt-manager.js",
            "maxSize": "3 kB",
            "compression": "brotli"
        }
    ],
    "nx": {
        "targets": {
            "build": {
                "outputs": [
                    "packages/@luvio/jwt-manager/dist"
                ]
            }
        }
    }
}
